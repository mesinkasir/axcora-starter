<div class="embed-{{#if ratio}}{{ratio}}{{else}}16x9{{/if}}">
  <video loading="lazy" title="{{title}}" controls>
    {{#if video_mp4}}
      <source src="{{video_mp4}}" type="video/mp4">
    {{/if}}
    {{#if video_webm}}
      <source src="{{video_webm}}" type="video/webm">
    {{/if}}
  </video>
</div>
